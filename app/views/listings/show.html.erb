<div class="page-header">
  <h3>Listing #<%= @listing.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Date</dt>
      <dd><%= @listing.date %></dd>

      <dt>Start time</dt>
      <dd><%= @listing.start_time %></dd>

      <dt>End time</dt>
      <dd><%= @listing.end_time %></dd>

      <dt>Recurring</dt>
      <dd><%= @listing.recurring %></dd>

      <dt>No of weeks</dt>
      <dd><%= @listing.no_of_weeks %></dd>

      <dt>Location</dt>
      <dd><%= @listing.location %></dd>

      <dt>User</dt>
      <dd><%= @listing.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/listings" class="btn btn-primary">
        Back
      </a>
      <a href="/listings/<%= @listing.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_listing/<%= @listing.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
