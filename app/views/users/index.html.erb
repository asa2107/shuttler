<div class="page-header">
  <h1>
    Users
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Gender</th>
        <th>Skill Level</th>
        <th>Email</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.gender %></td>
        <td><%= user.skill_level %></td>
        <td><%= user.email %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
